def ocurrencias(string):
    mensaje=""
    l=len(string)
    cont=0
    sum1=0
    sum0=0
    cadena=string
    while cont<l:
        if cadena[cont]=="1":
            sum1=sum1+1
        elif cadena[cont]=="0":
            sum0=sum0+1
        cont=cont+1
    if sum1>sum0:
        mensaje="1"
    elif sum1<sum0:
        mensaje="0"
    return mensaje
  
a="0101010101010111111111111111111110000001"
print(ocurrencias(a))